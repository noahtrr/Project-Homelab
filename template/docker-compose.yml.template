name: template
services:
  template:
    image: template/template:latest
    ports:
      - "80:80"
    restart: unless-stopped
    container_name: template
    hostname: template
    volumes:
      - hostPath:containerPath:ro
    environment:
      - name=value
    labels:
      tsdproxy.enable: "true"
      tsdproxy.name: "myserver"
      tsdproxy.autodetect: "false"
      tsdproxy.port: "80"
      tsdproxy.dash.icon: "sh/template"
      glance.name: Template
      glance.icon: sh:template
      glance.url: https://template.ojos-sole.ts.net
      glance.description: template
      glance.id: template
    networks:
      - proxy

networks:
  proxy:
    external: true